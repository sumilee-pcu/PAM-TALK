{
  "model_parameters": {
    "lookback_period": 30,
    "lstm_units": [
      64,
      32
    ],
    "dropout_rate": 0.2,
    "dense_units": 16,
    "activation": "relu",
    "output_activation": "linear"
  },
  "training_parameters": {
    "batch_size": 32,
    "epochs": 100,
    "validation_split": 0.2,
    "learning_rate": 0.001,
    "early_stopping_patience": 10,
    "reduce_lr_patience": 5
  },
  "data_parameters": {
    "training_days": 365,
    "test_split": 0.2,
    "features": [
      "demand",
      "price",
      "day_of_week",
      "month",
      "is_weekend",
      "is_holiday"
    ],
    "target": "demand"
  },
  "products": {
    "tomatoes": {
      "base_demand": 1000,
      "seasonal_amplitude": 300,
      "growth_rate": 0.0005,
      "noise_level": 0.15
    },
    "cabbage": {
      "base_demand": 800,
      "seasonal_amplitude": 200,
      "growth_rate": 0.0003,
      "noise_level": 0.12
    },
    "rice": {
      "base_demand": 2000,
      "seasonal_amplitude": 100,
      "growth_rate": 0.0002,
      "noise_level": 0.08
    }
  }
}